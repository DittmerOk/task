public class Main {


    public static void main(String[] args){

        Scanner scanner = new Scanner(System.in);
        int a = scanner.nextInt();
        System.out.print(a / 86400 + "d ");
        System.out.print(a % 86400/ 3600 + "h ");
        System.out.print(a % 3600 / 60 + "m ");
        System.out.print(a  % 60 + "s");


    }
}
