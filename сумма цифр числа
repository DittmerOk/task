class Main {
        public static void main(String[] args) {
            Scanner scanner = new Scanner(System.in);
            int number = scanner.nextInt();
            int sum = 0;
            int curentdigit;
             curentdigit = number % 10;
             sum = sum + curentdigit;

             number = number / 10;
             curentdigit = number % 10;
             sum = sum + curentdigit;
             number = number /10;
             sum = sum + number;

             System.out.println(sum);



        }
    }
